name: bilibili解锁高清
desc: 开启高码率及更高画质

http:
  mitm:
    - "api.bilibili.com"
  script:
    - match: https:\/\/ap(p|i)\.bilibili\.com\/((pgc\/player\/api\/playurl)|(x\/v2\/account\/myinfo\?)|(x\/v2\/account/mine\?))
      name: biliHD
      type: response
      require-body: true
      timeout: 10
      
script-providers:
  biliHD:
    url: https://raw.githubusercontent.com/Sunert/Script/master/Script/BiliHD.js
    interval:  86400
